{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-eval": "error",
      "no-restricted-globals": "error",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "strict": ["error", "never"]
    }
  }
  